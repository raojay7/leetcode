public class Solution {
    /**
     * @param A: An array of non-negative integers
     * @return: The maximum amount of money you can rob tonight
     */
    public long houseRobber(int[] A) {
        // write your code here
        int n=A.length;
        if (n==0)return 0;
        long []f=new long[n+1];
        f[0]=0;
        f[1]=A[0];
        for (int i=2;i<=n;++i){
            f[i]=Math.max(f[i-1],f[i-2]+A[i-1]);
        }
        return f[n];
    }
}
